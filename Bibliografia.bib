@PhdThesis{Denison1997,
  Title                    = {Simulation Based Bayesian Nonparametric Regression Methods},
  Author                   = {David George Taylor Denison},
  School                   = {Imperial College, London University},
  Year                     = {1997},
  Month                    = {September},
  Abstract                 = {This thesis introduces novel nonparametric Bayesian regression methods and utilises modern Markov chain Monte Carlo MCMC techniques to explore the posterior distributions of interest induced by the models. Traditional nonparametric regression approaches fit models using so called greedy algorithms which minimise some lack of fit criterion at each step. The algorithms are commonly divided into two parts: forward stepwise addition and backwards deletion. We demonstrate how by setting up a probability distribution over both the number and form of the basis functions we can improve on the greedy approach in terms of prediction and have more information to aid interpretation of the model as well. After an introduction describing both Bayesian and non Bayesian regression models we start by Bayesian curve fitting with various noise structures. The methodology is extended to perform spectral estimation in Chapter 3 and then multiple regression is considered in detail in Chapters 4 and 5. We introduce Bayesian versions of the classification and regression tree (CART) and the multivariate adaptive regression spline (MARS) algorithms. The Bayesian MARS model is then utilised in a variety of applications from nonlinear time series and financial data in Chapter5, to survival analysis with and without frailties in Chapter 6.},
  File                     = {Denison1997.pdf:Denison1997.pdf:PDF},
  Keywords                 = {Bayesian statistics, Nonparametrical Statistics},
  Owner                    = {Migue}
}

