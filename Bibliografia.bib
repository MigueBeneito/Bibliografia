@Article{Abellana.Ascaso2004,
  Title                    = {Métodos estadísticos para analizar el riesgo con patrones de distribución espacial},
  Author                   = {Abellana, Rosa and Ascaso, Carlos},
  Journal                  = {Medicina Cl\'{i}nica},
  Year                     = {2004},
  Pages                    = {68-72},
  Volume                   = {122},
  Abstract                 = {El desarrollo de los sistemas de información geográficos ha impulsado el interés por analizar las características de las poblaciones teniendo en cuenta el lugar donde viven. La salud de estas poblaciones, medidas como riesgos de morbilidad o mortalidad, está condicionada por una gran variedad de factores de riesgo característicos de su región, como los medioambientales, que en ocasiones no se pueden medir. Los modelos de análisis espacial recogen la dependencia de los indicadores de salud entre regiones próximas provocada por la existencia de los factores de riesgo que éstas comparten pero que a veces no están medidos, de tal manera que la distribución espacial de los indicadores depende del patrón geográfico que siguen los factores de riesgo. En este trabajo se comentan algunas de las limitaciones que presentan los métodos de estandarización y la regresión de Poisson para modelar la espacialidad y se muestran las ventajas que aportan los modelos espaciales autorregresivos. Esta metodología se ilustra mediante los datos de incidencia de diabetes tipo 1 en Cataluña durante el período 1989-1998.},
  File                     = {Abellana.Ascaso2044.pdf:Abellana.Ascaso2044.pdf:PDF},
  Keywords                 = {Disease mapping},
  Owner                    = {martinez_mig},
  Timestamp                = {2007.03.22}
}

